# File Managed by Puppet
[Unit]
Description=Iptables service
After=network.target ypbind.service nfs.service nfsserver.service rpcbind.service SuSEfirewall2_init.service

[Service]
ExecStart=/usr/sbin/iptables-restore <% if @preserve_rules_on_restore %>-n <% end %>/etc/sysconfig/iptables
ExecStop=/usr/sbin/iptables-restore /etc/sysconfig/iptables.stop
RemainAfterExit=true
Type=oneshot

[Install]
WantedBy=multi-user.target
